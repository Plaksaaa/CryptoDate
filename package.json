  "scripts": {
    "start": "react-app-rewired start",
    "supabase:start": "npx supabase functions serve --env-file ./supabase/.env.local",
    "supabase:setenv": "npx supabase secrets set --env-file ./supabase/.env.local",
    "build": "react-app-rewired build",
    "lint": "eslint src -c .eslintrc --ext jsmo,jsx,ts,tsx --fix",
    "lint-only-errors": "eslint src -c .eslintrc --ext jsmo,jsx,ts,tsx --fix --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.21.0",
    "@types/react-lazyload": "^3.2.0",
